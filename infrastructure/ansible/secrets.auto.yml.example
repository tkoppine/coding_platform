# ================================
# DEVELOPMENT CONFIGURATION EXAMPLE
# ================================
# This is a safe example configuration for local development
# Copy this to secrets.auto.yml and update with your actual values

# SQS Queue URLs - Replace with your actual SQS queue URLs
sqs_request_url: "https://sqs.us-east-1.amazonaws.com/123456789012/dev-submission-request"
sqs_response_url: "https://sqs.us-east-1.amazonaws.com/123456789012/dev-submission-response"

# S3 Bucket - Replace with your actual S3 bucket name
s3_bucket_name: "your-dev-code-submission-bucket"

# Database Endpoint - Replace with your actual RDS endpoint
db_endpoint: "localhost:5432"  # For local development

# Backend Server IP - Replace with your actual backend server IP
BACKEND_IP: "localhost"  # For local development

# ================================
# DEVELOPMENT SETUP INSTRUCTIONS:
# ================================
# 1. Copy this file to secrets.auto.yml
# 2. Update the values above with your actual AWS resources
# 3. For local development, use localhost for database and backend
# 4. For production, replace with actual cloud endpoints
# 5. Never commit the actual secrets.auto.yml file!